Xft.autohint  : 0
Xft.rgba      : rgb
Xft.hinting   : true
Xft.antialias : true
Xft.lcdfilter : lcddefault
Xft.hintstyle : hintslight
Xft.dpi       : 96

URxvt.letterSpace     : -1
URxvt.font            : xft:Inconsolata XL Medium:size=14
URxvt.perl-ext-common : default,matcher
URxvt.url-launcher    : /usr/bin/xdg-open
URxvt.matcher.button  : true
URxvt.intensityStyles : false
URxvt.saveLines       : 12000
URxvt.scrollBar       : false
URxvt.scrollstyle     : rxvt
URxvt.internalBorder  : 5

! special
*.foreground:   #433b32
*.background:   #dbd6d1
*.cursorColor:  #433b32

! black
*.color0:       #16130f
*.color8:       #5a5047

! red
*.color1:       #826d57
*.color9:       #826d57

! green
*.color2:       #57826d
*.color10:      #57826d

! yellow
*.color3:       #6d8257
*.color11:      #6d8257

! blue
*.color4:       #6d5782
*.color12:      #6d5782

! magenta
*.color5:       #82576d
*.color13:      #82576d

! cyan
*.color6:       #576d82
*.color14:      #576d82

! white
*.color7:       #a39a90
*.color15:      #dbd6d1

rofi.color-enabled : true
rofi.color-window  : #928374, #404552, #282828
rofi.color-normal  : #928374, #282828, #928374, #928374, #ffc200
rofi.color-active  : #928374, #282828, #928374, #928374, #00ff00
rofi.color-urgent  : #928374, #282828, #928374, #928374, #ff0000
